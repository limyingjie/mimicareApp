<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    android:layout_height="wrap_content"
    android:layout_width="match_parent"
    android:orientation="vertical" xmlns:android="http://schemas.android.com/apk/res/android">

    <TextView
        android:background="?android:attr/selectableItemBackground"
        android:id="@+id/setting_textView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/setting_margin_horizontal"
        android:layout_marginEnd="@dimen/setting_margin_horizontal"
        android:paddingTop="@dimen/setting_padding_vertical"
        android:paddingBottom="@dimen/setting_padding_vertical"
        android:paddingStart="@dimen/setting_padding_horizontal"
        android:paddingEnd="@dimen/setting_padding_horizontal"
        style="@style/TextProfileSetting"
        android:text="@string/setting" />

</LinearLayout>